class AddrAction extends Action {
	public function ip2geo($ip)
	{
		$record = geoip_record_by_name($ip);
		
		$country_name = "";
		$country_name = "";
		if($record)
		{
			$country_name = $record['country_name'];
			$city_name = $record['city'];
		}
		p($city_name."-".$country_name);
		return $city_name."-".$country_name;
	}
}
